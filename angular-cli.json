{
  "project": {
    "version": "1.0.0-beta.28.3",
    "name": "angular"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "favicon1.ico"
      ],
      "index": "index.html",
      "scripts": [
        "assets/js/jquery-2.2.3.min.js",
        "assets/js/alphaNav.min.js",
        "assets/js/velocity.min.js"
      ],
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "assets/css/skins/skin-fe.css",
        "assets/css/AdminLTE.css",
        "assets/css/angular-material.min.css",
        "assets/css/alphaNav.css",
        "assets/css/override.css"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "lint": [
    {
      "files": "src/**/*.ts",
      "project": "src/tsconfig.json"
    }
  ],
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
